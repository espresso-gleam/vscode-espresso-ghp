{
  "$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
  "name": "Espresso ghp",
  "scopeName": "source.gleam.ghp",
  "patterns": [
    {
      "begin": ">->",
      "beginCaptures": {
        "0": { "name": "punctuation.definition.embedded.html.begin.ghp" }
      },
      "end": "<-<",
      "endCaptures": {
        "0": { "name": "punctuation.definition.embedded.html.end.ghp" }
      },
      "contentName": "text.html.ghp",
      "patterns": [
        {
          "begin": "\\{",
          "beginCaptures": {
            "0": { "name": "punctuation.section.embedded.gleam.begin" }
          },
          "end": "\\}",
          "endCaptures": {
            "0": { "name": "punctuation.section.embedded.gleam.end" }
          },
          "contentName": "source.gleam.embedded.html",
          "patterns": [
            {
              "include": "source.gleam.ghp"
            }
          ]
        },
        {
          "include": "text.html.basic"
        }
      ]
    },
    {
      "include": "source.gleam"
    }
  ]
}